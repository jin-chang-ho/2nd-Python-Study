   "source": [
    "mem = [1, 2, 3]\n",
    "count = 0\n",
    "\n",
    "def coin(n):\n",
    "    global count\n",
    "    for i in mem:\n",
    "        if n == 0:\n",
    "            count = count + 1\n",
    "            return\n",
    "        if n - i < 0:\n",
    "            return\n",
    "        coin(n - i)\n",
    "            \n",
    "    \n",
    "n = int(input(\"n: \"))\n",
    "coin(n)\n",
    "print(count)\n",
    "    "
   ]
