    "number = int(input(\"n: \"))\n",
    "if number == 2:\n",
    "    print(\"소수\")\n",
    "else:\n",
    "    count = 0\n",
    "    for i in range(2, number):\n",
    "        if number % i == 0:\n",
    "            count = count + 1\n",
    "            print(\"소수 아님\")\n",
    "            break\n",
    "    if count == 0:\n",
    "        print(\"소수\")"
