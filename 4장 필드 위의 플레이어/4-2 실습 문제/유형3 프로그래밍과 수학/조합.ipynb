    "def factorial(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    return n * factorial(n-1)\n",
    "def c(number1, number2):\n",
    "    return factorial(number1) // (factorial(number2) * factorial(number1 - number2))\n",
    "\n",
    "n = int(input(\"n: \"))\n",
    "r = int(input(\"r: \"))\n",
    "print(\"결과:\", c(n,r))"
   ]
