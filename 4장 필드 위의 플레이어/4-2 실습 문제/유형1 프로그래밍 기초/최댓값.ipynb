   ],
   "source": [
    "i = 1\n",
    "numbers = input(\"여러 개의 숫자 입력: \").split()\n",
    "big = int(numbers[0])\n",
    "while True:\n",
    "    if i >= len(numbers):\n",
    "        break\n",
    "    number = int(numbers[i])\n",
    "    if (big < number):\n",
    "        big = number\n",
    "    i = i + 1\n",
    "print(\"가장 큰 숫자:\", big)"
   ]
