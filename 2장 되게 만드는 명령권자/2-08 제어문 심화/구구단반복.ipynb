      "4 x 8 = 32\n",
      "4 x 9 = 36\n",
      "몇 단?: x\n"
     ]
    }
   ],
   "source": [
    "while True:\n",
    "    number = input(\"몇 단?: \")\n",
    "    if number == 'x':\n",
    "        break\n",
    "    for i in range(1, 10):\n",
    "        print(int(number), 'x', i, '=', int(number) * i)"
   ]
